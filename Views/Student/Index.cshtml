@model List<Student1.Models.Student.StudentModel>
@{
}
<h1>Students List</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Diem</th>
            <th>Lop</th>
            <th>Date Of Birth</th>

        </tr>
    </thead>

    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Id</td>
                <td>@student.Name</td>
                <td>@student.Diem</td>
                <td>@student.Lop</td>
                <td>@student.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td><a href="Student/View/@student.Id">View</a></td>
            </tr>

        }
    </tbody>
</table>

